{
  "Lexer": {
    "TokensDefinitions": [
      {
        "TokenType": "UpperCaseWord",
        "Pattern": "^[A-Z][a-z]+('s|')?"
      },
      {
        "TokenType": "Word",
        "Pattern": "^[A-Za-z]+('s|')?"
      },
      {
        "TokenType": "Number",
        "Pattern": "^(\\d{1,3}((,\\d{3})*\\.?))(\\d*)"
      },
      {
        "TokenType": "Period",
        "Pattern": "^[\\.]"
      },
      {
        "TokenType": "Other",
        "Pattern": "^[^\\s]+"
      }
    ]
  }
}