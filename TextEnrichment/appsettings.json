{
  "Lexer": {
    "TokensDefinitions": [
      {
        "TokenType": "StringValue",
        "Pattern": "^[A-Za-z]+('s|')?"
      },
      {
        "TokenType": "Number",
        "Pattern": "^(\\d{1,3}((,\\d{3})*\\.?))(\\d*)"
      },
      {
        "TokenType": "Punctuation",
        "Pattern": "^[,\\.?!\"-:']"
      }
    ]
  },
  "DataFiles": {
    "TagExpressions": "Data/Tags.csv"
  }
}